#!/home/chris/Projects/interstate_sales/ENV/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'interstate-sales','console_scripts','initialize_interstate_sales_db'
__requires__ = 'interstate-sales'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('interstate-sales', 'console_scripts', 'initialize_interstate_sales_db')()
    )
